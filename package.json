{
  "name": "babel-project-relative-import",
  "version": "2.0.1",
  "repository": "per2plex/babel-project-relative-import",
  "description": "Babel plugin to transform project relative import paths to file relative import paths.",
  "keywords": [
    "babel",
    "path",
    "import",
    "relative",
    "project",
    "root",
    "windows"
  ],
  "main": "dist/plugin.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src/ -d dist/",
    "lint": "standard src/**/*.js test/**/*.js",
    "test": "npm run lint && mocha --compilers js:babel-core/register",
    "coverage": "babel-node $(npm bin)/isparta cover node_modules/.bin/_mocha",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "author": "Till Hagger <till.hagger@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "standard": "^6.0.8"
  },
  "dependencies": {
    "slash": "^1.0.0"
  }
}
